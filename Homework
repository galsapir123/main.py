 a = int(input('enter a number: '))

if a // 10 == a % 10:
    print('yes')
else:print('no')

#-------------------------------------------------------------

top = int(input('Enter a positive number:'))
num = 0
if top < 0:
    print('Negative number!')
else:
    while top > num:
        print(num)
        num = num + 1
        
        
